  <body>
    <div id="container">
      <div id="videoDemoContainer">
        <div id="videoControlsContainer">
          <div id="videoTitle">
            <h1></h1>
          </div><!-- #videoTitle -->
          <div id="videoControls">
            <div id="playControl">
              <div id="playButton">Play</div>
            </div><!-- #playControl -->
            <!-- <div id="rewindControl">
              <div id="rewindButton">Rewind</div>
            </div>#rewindControl -->
            <div id="scrubberControl">
              <div id="scrubberSlider">
                <div id="scrubberTime">0:00</div>
                <div id="scrubberDuration">0:00</div>
                <div id="scrubberSliderInner"></div>
              </div><!-- #scrubberSlider -->
            </div><!-- #scrubberControl -->
            <div id="volumeControl">
              <div id="volumeSlider">
                <div id="volumeSliderInner"></div>
              </div><!-- #volumeSlider -->
              <div id="volumeButton">Volume</div>
            </div><!-- #volume -->
            <div id="fullScreenControl">
              <div id="fullScreenButton">Full</div>
            </div><!-- #fullScreenControl -->
          </div><!-- #videoControls -->
        </div><!-- #videoControlsContainer -->
      </div><!-- #videoDemoContainer -->
      <div id="bcDemoContainer">
        <!-- Start of Brightcove Player -->

        <div style="display:none">

        </div>

        <!--
        By use of this code snippet, I agree to the Brightcove Publisher T and C
        found at https://accounts.brightcove.com/en/terms-and-conditions/.
        -->

        <script language="JavaScript" type="text/javascript" src="http://admin.brightcove.com/js/BrightcoveExperiences.js"></script>

        <object id="myExperience86176361001" class="BrightcoveExperience">
          <param name="bgcolor" value="#FFFFFF" />
          <param name="width" value="610" />
          <param name="height" value="343" />
          <param name="playerID" value="84961841001" />
          <param name="publisherID" value="78527748001"/>
          <param name="isVid" value="true" />
          <param name="isUI" value="true" />
          <param name="dynamicStreaming" value="true" />

          <param name="@videoPlayer" value="86176361001" />
        </object>

        <!--
        This script tag will cause the Brightcove Players defined above it to be created as soon
        as the line is read by the browser. If you wish to have the player instantiated only after
        the rest of the HTML is processed and the page load is complete, remove the line.
        -->
        <script type="text/javascript">brightcove.createExperiences();</script>

        <!-- End of Brightcove Player -->
      </div><!-- #bcDemoContainer -->
      <noscript>
        In order to view this video, you must have a browser that supports Javascript.
      </noscript>
    </div><!-- #container -->
