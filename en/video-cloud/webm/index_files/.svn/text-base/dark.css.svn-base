/* TimesPeople Dark */
#TP_container {
  background: #2b2b2b !important;
  border-bottom-color: #1c1c1c !important;
  color: #aaa !important;
}
#TP_container h4 {
  color: #aaa !important;
}
#TP_inner,
#TP_feed,
#TP_minimize_button,
.TP_toolbar_item {
  background-color: #2b2b2b;
  border-color: #414141;
}
.TP_toolbar_item {
  border-right-color: #1c1c1c !important;
}
#TP_inner {
  border-left-color: #1c1c1c !important;
}
#TP_feed_toggle {
  border-color: #414141 !important;
}
.TP_drawer_toggle {
  background-image: url(/images/blogs_v3/nyt_universal/dark/arrow-down.gif) !important;
}
#TP_feed_toggle.open {
  background-image: url(/images/apps/timespeople/arrow-up.gif) !important;
}
#TP_container a,
.TP_story a,
#TP_restore_button a {
  color: #999 !important;
}
.TP_story { 
  color: #666 !important;
}
#TP_container .TP_groove {
  background-image: url(/images/blogs_v3/nyt_universal/dark/handle.gif) !important;
}
#TP_container .open .TP_feed_content td {
  border-bottom: 1px solid #444 !important;
  background-color:#2b2b2b;
}
.open .TP_drawer_content {
  background-color:#2b2b2b;
}
.TP_feed_handle,
#TP_container .TP_drawer_tab {
  background-color: #444 !important;
}
.timespeople_btn_recommend .timespeople_recommend_link {
  background-image: url(/images/apps/timespeople/dark/recommend.gif) !important;
}

#TP_people button.appButtonSmall {
  background: url(/images/apps/timespeople/dark/rightcapsmall.png) right no-repeat !important;
}
#TP_people button.appButtonSmall span { 
  background: url(/images/apps/timespeople/dark/leftcapsmall.png) left no-repeat !important; 
}
ul#toolsList li a, ul#toolsList li a:visited {
  color: #999 !important;
}
#TP_container button.altSmall {
  color:#fff;
  background:url(/images/apps/buttons/rightcapsmall_dark.png) right no-repeat; 
}
#TP_container button.altSmall span {
  background:url(/images/apps/buttons/leftcapsmall_dark.png) left no-repeat; 
}
#TP_container button.appButtonSmallDone {
  background:url(/images/apps/buttons/rightcapsmall_dark_done.png) right no-repeat; 
}
#TP_container button.appButtonSmallDone span {
  background:url(/images/apps/buttons/leftcapsmall_dark_done.png) left no-repeat; 
}
.TP_drawer_tooltip,
.TP_drawer_tooltip_inner {
  background-color:#444 !important; 
}
#TP_minimize_button {
  background-image:url(/images/apps/timespeople/toolbar/1.5/close_dark.gif) !important;
}
.shadow {
  -webkit-box-shadow:2px 2px 7px #000;
  -moz-box-shadow:2px 2px 7px #000;
  box-shadow:2px 2px 7px #000;
  /* ie */
  -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Color=#000000, direction=135, strength=4 )";
  filter: progid:DXImageTransform.Microsoft.Shadow(Color=#000000, direction=135, strength=4 );
  border-left:1px solid #ccc\9;
  background-color: white\9;
  /* end ie */
}
#TP_container_shadow {
  -moz-box-shadow:2px 2px 7px #000;
  -webkit-box-shadow:2px 2px 7px #000;
  box-shadow:2px 2px 7px #000;
  /* ie */
  -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Color=#000000, direction=180, strength=4 )";
  filter: progid:DXImageTransform.Microsoft.Shadow(Color=#000000, direction=180, strength=4 );
  background-color: black\9;
  /* end ie */
}
.border-shadow {
  -moz-border-left-colors:   rgba(5,5,5,0.2) rgba(5,5,5, 0.4);
  -moz-border-right-colors:  rgba(5,5,5,0.1) rgba(5,5,5,0.2)  rgba(5,5,5,0.4) rgba(5,5,5, 0.6)  rgba(5,5,5, 0.7);
  -moz-border-bottom-colors: rgba(5,5,5,0.1) rgba(5,5,5,0.2)  rgba(5,5,5,0.4) rgba(5,5,5, 0.6)  rgba(5,5,5, 0.7);
}
#TP_feed.TP_toolbar_item_highlight,
.TP_toolbar_item_highlight {
  background-color:#444 !important;
}
.TP_annotation {
color:#999;
}
.TP_leftcap span {
  background-image: url(/images/apps/timespeople/buttons/leftcap_dark.png);
}


.TP_bevel span {
  background-image: url(/images/apps/timespeople/buttons/bevel_dark.png);
}

.TP_rightcap  {
  background-image: url(/images/apps/timespeople/buttons/rightcap_dark.png);
}
.TP_leftcap span,
.TP_bevel span,
.TP_rightcap span {
  color:#e2e2e2;
}
.TP_capsule_button {
  background-color: #555;
}

#TP_container textarea {
  background-color:#3d3d3d;
}

#TP_annotation_form {
  color:#AAAAAA;
}